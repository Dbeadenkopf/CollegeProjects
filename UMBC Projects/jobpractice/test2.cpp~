#include <iostream>
#include <fstream>
#include <vector>

using namespace std;


int main(){
  vector<string> U_name;
  vector<string> P_word;
  
  string u_name;
  string p_word;
  cout << "What is your user_name: " << endl;
  cin >> u_name;
  cout << "What is your Password: " << endl;
  cin >> p_word;



  cout << "Your username is: " << u_name << endl;
  cout << "Your password is: " << p_word << endl;

  U_name.push_back(u_name);
  P_word.push_back(p_word);

}
