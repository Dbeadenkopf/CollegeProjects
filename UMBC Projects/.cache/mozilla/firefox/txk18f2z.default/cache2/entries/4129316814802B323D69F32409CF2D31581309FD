<script type="text/javascript">
   
	var fg = 1;
	function update(){
		if(fg == 1){
			fg++;
			document.getElementById('nexxus').className = 'nexxus_white';
			document.getElementById('cta').className = 'cta_white';
		}else if(fg == 2){
			document.getElementById('nexxus').className = 'nexxus_black';
			document.getElementById('cta').className = 'cta_black';
		}
	}
	function isIE () {
	  var myNav = navigator.userAgent.toLowerCase();
	  return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;
	}
	if(!(isIE() == 9)){
		var dly = setTimeout(update,4000);
		var dly2 = setTimeout(update,13000);
	}
</script>
